{
  "name": "swr-vue",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "packageManager": "pnpm@6.11.0",
  "engines": {
    "node": "16"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/swr.umd.js",
  "module": "./dist/swr.es.js",
  "exports": {
    ".": {
      "import": "./dist/swr.es.js",
      "require": "./dist/swr.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "prepare": "husky install",
    "generate:release": "semantic-release",
    "preview": "vite preview",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest watch"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@edumudu/eslint-config": "^1.1.2",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.30.3",
    "@typescript-eslint/parser": "^5.30.3",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.1.4",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-testing-library": "^5.5.1",
    "eslint-plugin-vue": "^9.1.1",
    "flush-promises": "^1.0.2",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^2.9.13",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.16.0",
    "vue": "^3.2.33"
  },
  "peerDependencies": {
    "vue": "^3.2.33"
  },
  "dependencies": {
    "@vueuse/core": "^8.7.5"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix"
  },
  "publishConfig": {
    "access": "public"
  }
}
