{
  "root": true,
  "extends": "@edumudu/eslint-config/vue-ts",
  "parserOptions": {
    "project": "./tsconfig.eslint.json"
  },
  "rules": {
    "import/order": ["error", {
        "newlines-between": "always",
        "groups": [
          "builtin",
          "external",
          "internal",
          ["parent", "sibling", "index"]
        ]
      }
    ]
  }
}
